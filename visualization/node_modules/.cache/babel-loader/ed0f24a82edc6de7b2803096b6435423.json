    this.clearMessages();\n    });\n\n  },\n  methods: {\n    clearMessages: function() {\n      setInterval(() => {\n        this.messages = [];\n      }, 5000);\n    },\n  }\n};\n</script>"],"sourceRoot":"src/components"}]}